{
  "eslint.workingDirectories": [
    { "directory": "backend", "changeProcessCWD": true }
  ],
  "eslint.useFlatConfig": true,
  "eslint.validate": [
    "javascript",
    "typescript",
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.formatOnSave": false
}
